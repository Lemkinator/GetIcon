<?xml version="1.0" encoding="utf-8"?>
<resources xmlns:tools="http://schemas.android.com/tools">

    <style name="Theme.GetIcon" parent="OneUITheme">
        <item name="colorPrimary">@color/primary_color_themed</item>
        <item name="colorPrimaryDark">@color/primary_dark_color_themed</item>
        <item name="android:windowContentTransitions">true</item>
        <item name="android:windowOptOutEdgeToEdgeEnforcement" tools:ignore="NewApi">true</item>
    </style>

    <style name="Theme.GetIcon.Splash" parent="Theme.SplashScreen">
        <item name="windowSplashScreenBackground">@color/oui_round_and_bgcolor</item>
        <item name="windowSplashScreenAnimatedIcon">@drawable/ic_splash</item>
        <item name="windowSplashScreenAnimationDuration">700</item>
        <item name="postSplashScreenTheme">@style/Theme.GetIcon</item>
    </style>

</resources>